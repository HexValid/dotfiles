Xft.dpi: 120
!Xft.autohint: 0
!Xft.lcdfilter:  lcddefault
!Xft.hintstyle:  hintfull
!Xft.hinting: 1
!Xft.antialias: 1
!Xft.rgba: rgb
URxvt.font: xft:IBM Plex Mono Medium:size=11, xft:Symbola:size=11
